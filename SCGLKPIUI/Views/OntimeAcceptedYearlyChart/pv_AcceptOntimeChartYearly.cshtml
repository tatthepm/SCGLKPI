<div class="content animate-panel">
    <div class="row">
        @*graph#1*@
        <div class="col-lg-6">
            <div class="hpanel">
                <div class="panel-heading">
                    <div class="panel-tools">
                        <a class="showhide"><i class="fa fa-chevron-up"></i></a>
                        <a class="closebox"><i class="fa fa-times"></i></a>
                    </div>
                    On-time accept tender
                </div>
                <div class="panel-body">
                    <div>
                        <h5 style="text-align:center">On-time accept tender</h5>
                        <canvas id="barOntimeAcceptYearly" height="140"></canvas>
                    </div>
                    <div id="js-legend" class="chart-legend"></div>
                </div>
            </div>
        </div>
        @*graph#3*@
        <div class="col-lg-6">
            <div class="hpanel">
                <div class="panel-heading">
                    <div class="panel-tools">
                        <a class="showhide"><i class="fa fa-chevron-up"></i></a>
                        <a class="closebox"><i class="fa fa-times"></i></a>
                    </div>
                    On-time accept tender adjusted
                </div>
                <div class="panel-body">
                    <div>
                        <h5 style="text-align:center">On-time accept tender adjusted</h5>
                        <canvas id="barOntimeAcceptAdjustYearly" height="140"></canvas>
                    </div>
                    <div id="js-legend2" class="chart-legend"></div>
                </div>
            </div>
        </div>

    </div>
    <div class="row">
        @*graph#2*@
        <div class="col-lg-6">
            <div class="hpanel">
                <div class="panel-heading">
                    <div class="panel-tools">
                        <a class="showhide"><i class="fa fa-chevron-up"></i></a>
                        <a class="closebox"><i class="fa fa-times"></i></a>
                    </div>
                    Shipments Amount
                </div>
                <div class="panel-body">
                    <div>
                        <h5 style="text-align:center">Shipments per year</h5>
