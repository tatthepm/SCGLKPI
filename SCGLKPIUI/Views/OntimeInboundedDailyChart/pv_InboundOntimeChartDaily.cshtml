<div class="content animate-panel">
    <div class="row">
        @*graph#1*@
        <div class="col-lg-6">
            <div class="hpanel">
                <div class="panel-heading">
                    <div class="panel-tools">
                        <a class="showhide"><i class="fa fa-chevron-up"></i></a>
                        <a class="closebox"><i class="fa fa-times"></i></a>
                    </div>
                    On-time inbound
                </div>
                <div class="panel-body">
                    <div>
                        <h5 style="text-align:center">On-time inbound </h5>
                        <canvas id="barOntimeInbound" height="140"></canvas>
                    </div>
                    <div id="js-legend" class="chart-legend"></div>
                </div>
            </div>
        </div>
        @*graph#3*@
        <div class="col-lg-6">
            <div class="hpanel">
                <div class="panel-heading">
                    <div class="panel-tools">
                        <a class="showhide"><i class="fa fa-chevron-up"></i></a>
                        <a class="closebox"><i class="fa fa-times"></i></a>
                    </div>
                    On-time inbound adjusted
                </div>
                <div class="panel-body">
                    <div>
                        <h5 style="text-align:center">On-time inbound adjusted</h5>
                        <canvas id="barOntimeInboundAdjust" height="140"></canvas>
                    </div>
                    <div id="js-legend2" class="chart-legend"></div>
                </div>
            </div>
        </div>

    </div>
    <div class="row">
        @*graph#2*@
        <div class="col-lg-6">
            <div class="hpanel">
                <div class="panel-heading">
                    <div class="panel-tools">
                        <a class="showhide"><i class="fa fa-chevron-up"></i></a>
                        <a class="closebox"><i class="fa fa-times"></i></a>
                    </div>
                    DN Amounts
                </div>
                <div class="panel-body">
                    <div>
                        <h5 style="text-align:center">DN per day</h5>
                        <canvas id="barChartShipment" height="140"></canvas>
                    </div>
                    <div id="js-legend3" class="chart-legend"></div>
                </div>
            </div>
        </div>
        @*graph#4*@
        <div class="col-lg-6">
            <div class="hpanel">
                <div class="panel-heading">
                    <div class="panel-tools">
                        <a class="showhide"><i class="fa fa-chevron-up"></i></a>
                        <a class="closebox"><i class="fa fa-times"></i></a>
                    </div>
                    Percentage of DN
                </div>
                <div class="panel-body">
                    <div>
                        <h5 style="text-align:center">Percentage of DN categorize by product </h5>
                        <canvas id="doughnutChart" height="140"></canvas>
                    </div>
                    <div id="js-legend4" class="chart-legend"></div>
                </div>
            </div>
        </div>
    </div>
</div>
